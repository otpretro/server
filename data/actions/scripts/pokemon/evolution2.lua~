local types = {



["sh"] = {itemid = 2361, "Salamence", "Tangrowth", "Milotic", "Charizard", "Alakazam", "Dragonite", "Blastoise", "Vileplume", "Venomoth", "Aerodactyl", "Gengar", "Feraligatr", "Typhlosion", "Meganium", "Tropius","Metagross"},
